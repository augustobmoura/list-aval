    "/api/Lista/filtro": {
      "get": {
        "tags": [
          "Lista"
        ],
        "summary": "Filtro de lista. Funciona de forma cumulativa. Se eu escolher nivel de dificuldade 'Fácil' e Tempo Máximo de Resposta 10 minutos, então \r\no a api só retornar as listas que atendem aos dois filtros. Não é preciso preencher todos os filtros, somente àqueles que se deseja usar.",
        "operationId": "ConsulteListaComFiltro",
        "consumes": [],
        "produces": [
          "text/plain",
          "application/json",
          "text/json"
        ],
        "parameters": [
          {
            "name": "nivelDificuldade",
            "in": "query",
            "description": "1 - 'Muito Fácil'; 2 - 'Fácil', 3 - 'Médio', 4 - 'Difícil', 5 - 'Muito Difícil'",
            "required": false,
            "type": "integer",
            "format": "int32",
            "default": -1
          },
          {
            "name": "disciplina",
            "in": "query",
            "description": "Deve-se passar o código da disciplina desejada. O filtro busca por listas que possuem questões dessa disciplina.",
            "required": false,
            "type": "string"
          },
          {
            "name": "areaDeConhecimento",
            "in": "query",
            "description": "Deve-se passar o código da área de conhecimento desejada. O filtro busca por listas que possuem questões dessa área de conhecimento.",
            "required": false,
            "type": "string"
          },
          {
            "name": "tempoEsperadoResposta",
            "in": "query",
            "description": "Tempo máximo de resposta da lista expresso em minutos.",
            "required": false,
            "type": "integer",
            "format": "int32",
            "default": 0
          },
          {
            "name": "usuario",
            "in": "query",
            "description": "Autor da lista.",
            "required": false,
            "type": "string"
          },
          {
            "name": "id",
            "in": "query",
            "description": "O id da lista, caso precise só de uma.",
            "required": false,
            "type": "string"
          },
          {
            "name": "tags",
            "in": "query",
            "description": "Busca as tags das questões e traz as listas que possuem questões com as tags passada.",
            "required": false,
            "type": "array",
            "items": {
              "type": "string"
            },
            "collectionFormat": "multi",
            "uniqueItems": false
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "$ref": "#/definitions/DtoResultado[DtoListaQuestoes]"
            }
          }
        }
      }
    },
