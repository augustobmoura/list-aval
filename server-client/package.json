{
	"name": "server-client",
	"version": "1.0.0",
	"main": "dist/index.js",
	"module": "dist/index.js",
	"license": "MIT",
	"scripts": {
		"refresh-spec": "download http://localhost:8080/v3/api-docs.yaml > spec.yaml",
		"build:spec": "java -jar openapi-generator-cli.jar generate -i spec.yaml -g typescript-angular -o generated-src",
		"build:ts:ng": "ngc",
		"build:ts:rm": "rimraf dist/node_modules",
		"build:ts:mv": "mv dist/generated-src/* dist",
		"build:ts": "npm-run-all build:ts:ng build:ts:rm build:ts:mv",
		"build": "npm-run-all build:spec build:ts",
		"clean": "rimraf dist generated-src",
		"prepack": "npm-run-all clean build"
	},
	"devDependencies": {
		"@angular/common": "^8.2.12",
		"@angular/compiler": "^8.2.12",
		"@angular/compiler-cli": "^8.2.12",
		"@angular/core": "^8.2.12",
		"download-cli": "^1.1.1",
		"npm-run-all": "^4.1.5",
		"rimraf": "^3.0.0",
		"rxjs": "^6.4.0",
		"typescript": "~3.5.0",
		"zone.js": "~0.9.1"
	},
	"peerDependencies": {
		"@angular/common": "^8.2.12",
		"@angular/core": "^8.2.12",
		"rxjs": "^6.4.0",
		"zone.js": "~0.9.1"
	}
}
